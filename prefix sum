class Solution:
    def pivotIndex(self, nums: List[int]) -> int:
        prefix=[-1]*len(nums)
        prefix[0]=nums[0]
        for i in range(1,len(nums)):
            prefix[i]=prefix[i-1]+nums[i]
        
        sufix=[-1]*len(nums)
        sufix[-1]=nums[-1]
        for i in range(len(nums)-2,-1,-1):
            sufix[i]=sufix[i+1]+nums[i]
        
        for i in range(len(nums)):
            if prefix[i]==sufix[i]:
                return i
        return -1
